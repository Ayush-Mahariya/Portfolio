<div class="col-md-4 col-sm-6">
    <div class="card mb-5">
       <div class="card-header has-icon">
            <i class=<%=classType%> aria-hidden="true"></i>
        </div>
        <div class="card-body px-4 py-3">
            <h5 class="mb-3 card-title text-dark"><%=headingTxt%></h5>
            <p>Site: <a href="<%=siteLink%>"><%=siteLink%></a></p>
            <p>Github Repository: <a href="<%=gitLink%>">Open<i class="ti-link text-danger" aria-hidden="true"></i></a></p>
            <p>Used: 
                <%skillList.forEach((skill) => {%>
                    <button class="btn btn-dark btn-sm my-1"><%=skill%></button>
                <%});%>
            </p>
            <P class="subtitle"><%=paraTxt%></P>
        </div>
    </div>
</div>